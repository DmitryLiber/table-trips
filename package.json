{
  "name": "table-tips",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "eslint": "eslint src/**/*.js",
    "eslint:fix": "eslint src/**/*.js --fix",
    "start": "set NODE_ENV=development & concurrently \"vite --host\" \"npx gulp renameStaticFoldersDev && npx gulp preDev\" \"npx gulp watcherFiles\"",
    "build": "set NODE_ENV=production&& npx gulp preBuild && npx gulp renameStaticFoldersBuild && vite build --logLevel error && npx gulp afterBuild",
    "preview": "vite preview",
    "img": "npx gulp img",
    "test": "npm run build && npm run stylelint && npm run eslint && npx gulp test"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@hail2u/css-mqpacker": "github:hail2u/node-css-mqpacker#v9.0.1",
    "@prettier/plugin-pug": "2.5.1",
    "@vituum/vite-plugin-postcss": "^1.0.0",
    "@vituum/vite-plugin-pug": "^1.0.0",
    "concurrently": "^8.2.0",
    "cpy": "10.1.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-copy": "^1.1.0",
    "gulp-html-beautify": "^1.0.1",
    "gulp-html-bem-validator": "1.0.5",
    "gulp-imagemin": "8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "2.0.0",
    "gulp-w3c-html-validator": "5.2.0",
    "husky": "8.0.3",
    "imagemin-webp": "8.0.0",
    "lint-staged": "13.2.3",
    "mkdirp": "3.0.1",
    "postcss": "^8.4.13",
    "sass": "^1.63.6",
    "stylelint": "^14.12.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.32"
  },
  "dependencies": {
    "swiper": "9.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 25 versions",
    "defaults and supports es6-module"
  ]
}
